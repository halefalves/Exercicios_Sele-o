Algoritmo "Exercicio 1"
// Nome: Halef Alves de Olivira
// CPD: 34546

Var
   n1:inteiro


Inicio

   escreval ("Digite um numero:")
   leia (n1)

   se (n1%2=0) entao
      escreval ("Numero par!")
   senao
      escreval ("Numero impar!")
   fimse

   se (n1 >= 0) entao
      escreval ("Positivo")
   senao
      escreval ("Negativo")
   fimse



Fimalgoritmo